{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "strict": true,
    "module": "commonjs",
    "target": "es5",
    "downlevelIteration": true,
    "lib": ["es5", "es6", "dom"],
    "jsx": "react",
    "esModuleInterop": true
  },
  "include": [
    "./components/**/*",
    "./entrypoints/**/*",
    "./redux/**/*",
    "./shared/**/*",
    "./types/**/*"
  ]
}
